{
  "printWidth": 90,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "overrides": [
    {
      "files": ["**/*.scss"],
      "options": {
        "singleQuote": false,
        "number-leading-zero": "never"
      }
    },
    {
      "files": ["*.tsx", "*.ts"],
      "options": {
        "parser": "typescript"
      }
    }
  ]
}
